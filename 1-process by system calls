#include <stdio.h>
#include <unistd.h>

int main() {
    pid_t process_id;
    pid_t parent_id;

    printf("Enter the process ID: ");
    scanf("%d", &process_id);

    printf("Enter the parent process ID: ");
    scanf("%d", &parent_id);

    printf("Process ID: %d\n", process_id);
    printf("Parent Process ID: %d\n", parent_id);

    return 0;
}

